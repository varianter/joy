datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Videos {
  id     String @id @default(cuid())
  title    String
  description String
  uri String
  isSuggested Boolean  @default(false)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}